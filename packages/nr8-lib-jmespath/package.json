    "@types/async": "^3.2.8",
    "@types/jest": "^27.0.3",
    "@types/lodash": "^4.14.175",
    "@types/node": "^14.14.43",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "babel-eslint": "^10.1.0",
